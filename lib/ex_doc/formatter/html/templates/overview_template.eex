    <%= head_template(config, %{id: :overview}) %>
    <%= sidebar_template(config, modules, exceptions, protocols) %>

      <h1><%= config.project %> v<%= config.version %></h1>

      <ul class="summary-links">
        <%= unless Enum.empty?(modules) do %>
          <li><a href="#modules">Modules</a></li>
        <% end %>
        <%= unless Enum.empty?(exceptions) do %>
          <li><a href="#exceptions">Exceptions</a></li>
        <% end %>
        <%= unless Enum.empty?(protocols) do %>
          <li><a href="#protocols">Protocols</a></li>
        <% end %>
      </ul>

      <%= unless Enum.empty?(modules) do %>
        <section id="modules" class="details-list">
          <h1 class="section-heading">Modules summary</h1>
          <div class="summary">
            <%= for node <- modules, do: overview_entry_template(node) %>
          </div>
        </section>
      <% end %>

      <%= unless Enum.empty?(exceptions) do %>
        <section id="exceptions" class="details-list">
          <h1 class="section-heading">Exceptions summary</h1>
          <div class="summary">
            <%= for node <- exceptions, do: overview_entry_template(node) %>
          </div>
        </section>
      <% end %>

      <%= unless Enum.empty?(protocols) do %>
        <section id="protocols" class="details-list">
          <h1 class="section-heading">Protocols summary</h1>
          <div class="summary">
            <%= for node <- protocols, do: overview_entry_template(node) %>
          </div>
        </section>
      <% end %>
    <%= footer_template %>
